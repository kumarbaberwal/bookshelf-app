{
  "expo": {
    "name": "BookShelf",
    "slug": "bookshelf",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/i.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kumarbaberwal.bookshelf",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/i.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kumarbaberwal.bookshelf"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/i.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/i.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4f41c75c-4478-416d-9156-bbdc964e5da7"
      }
    }
  }
}